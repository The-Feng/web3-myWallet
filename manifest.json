{
  "manifest_version": 3,
  "name": "My wallet Plugin",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.ts"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.ts"
      ]
    }
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}